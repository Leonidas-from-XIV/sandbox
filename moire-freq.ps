% (frequency -- )
/freqlines
  { %def
	  0 exch 400 { %for
		  dup
		  0 moveto
		  250 lineto
		  stroke
	  } for
  } def

gsave
	10 500 translate
	0.95 4 mul freqlines
grestore
gsave
	50 400 translate
	4 freqlines
grestore
